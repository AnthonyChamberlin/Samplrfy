<div class="col-md-10 col-md-offset-1 login_panel">
<%= simple_form_for @track, :html => { :multipart => true } do |f| %>
<%= f.input :title, label: "Track Name:" %>
<%= f.file_field :mp3 %>
<%= f.select :genre_id, @genres, class: "genre_select" %>
<%= f.input :price %>
<%= f.input :description %>
<%= f.button :submit %>
<% end %>

<%= link_to "Back", track_path(@track), class: "btn btn-primary" %>

</div>
