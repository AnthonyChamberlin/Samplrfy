<div class="col-sm-10 col-md-offset-1">
  <div class="container-fluid login_panel">
    <h1>Edit Review</h1>

    <%= simple_form_for([@track, @review]) do |f| %>
    <div id="rating-form">
      <label>Rating |  </label>
    </div>
      <%= f.input :comment %>
      <%= f.button :submit %>
    <% end %>
  </div>
</div>

<script>
  $('#rating-form').raty({
    path: '/assets/',
    scoreName: 'review[rating]'
  });
</script>
